<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
	"http://java.sun.com/dtd/web-facesconfig_1_0.dtd">

<!-- This file generated by Sakai App Builder -AZ -->
<faces-config>

	<!-- Message Bundle -->
     <application>
        <message-bundle>org.sakaiproject.tool.videoconference.messages</message-bundle>
        <locale-config>
            <default-locale>en</default-locale>
        </locale-config>
    </application>

	<!-- Navigation Rules -->
    <navigation-rule>
		<from-view-id>/videoconference/VideoconferenceRoomList.jsp</from-view-id>
		<navigation-case>
			<from-outcome>editRoom</from-outcome>
			<to-view-id>/videoconference/VideoconferenceAddRoom.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>accessRoom</from-outcome>
			<to-view-id>/videoconference/VideoconferenceLoadingRoom.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>confirmDeleteRooms</from-outcome>
			<to-view-id>/videoconference/VideoconferenceConfirmDeleteRooms.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>checkStatus</from-outcome>
			<to-view-id>/videoconference/VideoconferenceCheckStatus.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/videoconference/VideoconferenceAddRoom.jsp</from-view-id>
		<navigation-case>
			<from-outcome>roomList</from-outcome>
			<to-view-id>/videoconference/AuxiliarPage.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>error</from-outcome>
			<to-view-id>/videoconference/VideoconferenceAddRoom.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/videoconference/VideoconferenceConfirmDeleteRooms.jsp</from-view-id>
		<navigation-case>
			<from-outcome>roomList</from-outcome>
			<to-view-id>/videoconference/AuxiliarPage.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/videoconference/VideoconferenceLoadingRoom.jsp</from-view-id>
		<navigation-case>
			<from-outcome>roomList</from-outcome>
			<to-view-id>/videoconference/AuxiliarPage.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/videoconference/VideoconferenceCheckStatus.jsp</from-view-id>
		<navigation-case>
			<from-outcome>checkStatus</from-outcome>
			<to-view-id>/videoconference/VideoconferenceCheckStatus.jsp</to-view-id>
		</navigation-case>
		<navigation-case>
			<from-outcome>roomList</from-outcome>
			<to-view-id>/videoconference/AuxiliarPage.jsp</to-view-id>
		</navigation-case>		
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/videoconference/AuxiliarPage.jsp</from-view-id>
		<navigation-case>
			<from-outcome>roomList</from-outcome>
			<to-view-id>/videoconference/VideoconferenceRoomList.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

    <!-- Backing Bean -->
    <managed-bean>
        <description>Videoconference Openmeetings Tool</description>
        <managed-bean-name>VideoconferenceBean</managed-bean-name>
        <managed-bean-class>org.sakaiproject.videoconference.openmeetings.tool.jsf.VideoconferenceBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<description>Logic</description>
			<property-name>videoconferenceLogic</property-name>
			<value>#{org_sakaiproject_videoconference_openmeetings_logic_VideoconferenceLogic}</value>
		</managed-property>
    </managed-bean>  

</faces-config>