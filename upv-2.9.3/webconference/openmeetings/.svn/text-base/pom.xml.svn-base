   <dependencies>

	  <!-- internal dependencies -->
      <dependency>
         <groupId>org.sakaiproject.videoconference</groupId>
         <artifactId>videoconference-api</artifactId>
		 <version>${samoo.videoconference.version}</version>
      </dependency>
	  <dependency>
         <groupId>org.sakaiproject.videoconference</groupId>
         <artifactId>videoconference-common</artifactId>
		 <version>${samoo.videoconference.version}</version>
      </dependency>
	  
      <!-- Sakai dependencies -->
    	<dependency>
            <groupId>org.sakaiproject.kernel</groupId>
            <artifactId>sakai-kernel-api</artifactId>
